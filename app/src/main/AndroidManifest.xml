<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.edu.unab.formsaludapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FormSaludApp">
        <activity
            android:name=".vista.ListadoMetabolismo"
            android:exported="true" />
        <activity
            android:name=".vista.ListadoMasaCorporal"
            android:exported="true" />
        <activity
            android:name=".vista.CalcularMetabolismo"
            android:exported="false" />
        <activity
            android:name=".vista.CalcularMasa"
            android:exported="false" />
        <activity
            android:name=".vista.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>